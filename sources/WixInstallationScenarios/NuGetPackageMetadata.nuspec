        <id>$id$</id>
        <version>1.0.2</version>
        <title>$id$</title>
        <authors>Dust in the Wind</authors>
        <owners>Dust in the Wind</owners>
        <licenseUrl>http://LICENSE_URL_HERE_OR_DELETE_THIS_LINE</licenseUrl>
        <projectUrl>http://PROJECT_URL_HERE_OR_DELETE_THIS_LINE</projectUrl>
        <iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A WiX Toolset library containing properties to identify the installation scenarios.</description>
        <copyright>Copyright Â© Dust in the Wind 2021</copyright>
        <tags>installer windows-installer wix wix-toolset</tags>
    </metadata>
</package>